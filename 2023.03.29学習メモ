// 問39
// 全ての試合のキックオフ時間と対戦国の国名を表⽰してください。
//   pairings kickoff id my_country_id enemy_country_id
//  countries name id

SELECT p.kickoff AS "キックオフ時間", c1.name AS "対戦国1", c2.name AS "対戦国2"
FROM pairings p 
LEFT JOIN countries c1 ON c1.id = my_country_id
LEFT JOIN countries c2 ON c2.id = enemy_country_id ;

SELECT p.kickoff AS "キックオフ時間", c1.name AS "対戦国1", c2.name AS "対戦国2"
FROM pairings p 
JOIN countries c1 ON c1.id = my_country_id
JOIN countries c2 ON c2.id = enemy_country_id ;

SELECT p.kickoff AS "キックオフ時間", c1.name AS "対戦国1", c2.name AS "対戦国2"
FROM pairings p 
RIGHT JOIN countries c1 ON c1.id = my_country_id
RIGHT JOIN countries c2 ON c2.id = enemy_country_id ;

+---------------------+--------------------------+--------------------------+
| キックオフ時間      | 対戦国1                  | 対戦国2                  |
+---------------------+--------------------------+--------------------------+
| 2014-06-13 05:00:00 | ブラジル                 | クロアチア               |
| 2014-06-14 01:00:00 | メキシコ                 | カメルーン               |
| 2014-06-14 04:00:00 | スペイン                 | オランダ                 |
